local tagtable = ''
emojitag = emojitag:gsub(':','')
local unicodesplit = emojiunicode:split(' ')
local tagsplit = emojitag:split(' ')
for tag = 1, #tagsplit do
	tagtable = tagtable..[[

["]]..tagsplit[tag]..[["] = ']]..unicodesplit[tag]..[[';]]
end 
print(tagtable)